{
  "name": "cli_ghp_deployer",
  "version": "1.0.4",
  "description": "Написать консольное приложение для публикации страниц на github",
  "main": "dist/src/index.js",
  "bin": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc -b",
    "start": "node dist/src/index.js",
    "test_cmd": "echo my super build",
    "lint": "eslint . && prettier --check .",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint . --fix && prettier --write .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "deploy": "ts-node  src/index.ts  ghp-deploy -d deploy_dir -r test_repo_for_publish "
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vasiliev-alexey/cli_ghp_deployer.git"
  },
  "keywords": [
    "github-pages"
  ],
  "author": "vasiliev-alexey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vasiliev-alexey/cli_ghp_deployer"
  },
  "homepage": "https://github.com/vasiliev-alexey/cli_ghp_deployer#readme",
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^7.3.3",
    "commander": "^7.2.0",
    "gh-pages": "^3.2.3",
    "git-user-name": "^2.0.0",
    "inquirer": "^8.1.2",
    "lodash": "^4.17.21",
    "tslog": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/gh-pages": "^3.2.0",
    "@types/git-user-name": "^2.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "tslib": "^1.14.1",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,md,json,yml,html}": "prettier --write"
  }
}
